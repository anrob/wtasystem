<%= form_for(resource, as: resource_name, url: session_path(resource_name),html: { class: "well form-well" }) do |f| %>

    <%= f.label :actcode %>
        <%= f.text_field :actcode_name,  class: "username" %>
         <span class="help-inline">Actcode must be in all CAPS!</span>
        <%= f.label :password %>
        <%= f.password_field :password,  class: "password" %>
    <br /><br />
        <%= f.submit "  Login  ", class: "btn-primary" %>
         <!--loginbox_content-->
    <%= render partial: "devise/shared/links" %><!--loginbox_inner-->
<!--loginbox-->
<% end %>